{
  "name": "geekcom/phpjasper",
  "description": "A PHP report generator",
  "license": "MIT",
  "keywords": [
    "reports",
    "php-reports",
    "pdf"
  ],
  "authors": [
    {
      "name": "Daniel Rodrigues Lima",
      "email": "danielrodrigues-ti@hotmail.com",
      "role": "lead"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1",
    "symfony/console": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
